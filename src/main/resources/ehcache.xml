<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns='http://www.ehcache.org/v3'
         xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd">

    <cache-template name="simple">
        <expiry>
            <ttl unit="seconds">3600</ttl>
        </expiry>
        <heap>100</heap>
    </cache-template>

    <cache alias="com.mfernandes.app.domain.User" uses-template="simple"/>

    <cache alias="com.mfernandes.app.domain.Authority" uses-template="simple"/>

    <cache alias="com.mfernandes.app.domain.User.authorities" uses-template="simple"/>

    <cache alias="com.mfernandes.app.domain.PersistentToken" uses-template="simple"/>

    <cache alias="com.mfernandes.app.domain.User.persistentTokens" uses-template="simple"/>

    <cache alias="com.mfernandes.app.domain.TBBase" uses-template="simple"/>

    <cache alias="com.mfernandes.app.domain.TBModeloExclusivo" uses-template="simple"/>

    <cache alias="com.mfernandes.app.domain.TBModeloGenerico" uses-template="simple"/>

    <cache alias="com.mfernandes.app.domain.TBProjeto" uses-template="simple"/>

    <cache alias="com.mfernandes.app.domain.TBProjeto.referencias" uses-template="simple"/>

    <cache alias="com.mfernandes.app.domain.TBProjeto.mantems" uses-template="simple"/>

    <cache alias="com.mfernandes.app.domain.TBSessao" uses-template="simple"/>

    <cache alias="com.mfernandes.app.domain.TBSessao.associas" uses-template="simple"/>

    <cache alias="com.mfernandes.app.domain.TBUsuario" uses-template="simple"/>

    <cache alias="com.mfernandes.app.domain.TBUsuario.usas" uses-template="simple"/>

    <cache alias="com.mfernandes.app.domain.TBUsuario.possuis" uses-template="simple"/>

    <cache alias="com.mfernandes.app.domain.TBUsuario.armazenas" uses-template="simple"/>

    <!-- jhipster-needle-ehcache-add-entry -->
</config>
